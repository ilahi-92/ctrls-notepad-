SELECT table_name "table size",
        ROUND(SUM(data_length + index_length) / 1024 / 1024/1024, 3) "DB Size in GB" 
FROM information_schema.tables 
WHERE table_schema in ('zabbix')
GROUP BY table_name
ORDER BY 2 DESC LIMIT 10;

