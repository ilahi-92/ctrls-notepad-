    TABLE_NAME, 
    PARTITION_NAME, 
    TABLE_ROWS
FROM information_schema.PARTITIONS 
WHERE TABLE_SCHEMA = 'zabbix' 
    AND TABLE_NAME = 'history_uint'
    AND PARTITION_NAME IS NOT NULL
